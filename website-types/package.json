{
  "name": "3xr_types",
  "version": "1.0.0",
  "description": "Shared 3XR types",
  "author": "Mike Festa",
  "homepage": "https://www.3xr.com",
  "license": "SPDX-License-Identifier: Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/MikeFesta/3xr.git"
  },
  "scripts": {
    "prettier": "prettier --write .",
    "tsc": "node_modules/typescript/bin/tsc",
    "build-enums": "./node_modules/typescript/bin/tsc enums.ts"
  },
  "prettier": {
    "singleQuote": true,
    "semi": true,
    "trailingComma": "all",
    "printWidth": 120,
    "tabWidth": 2,
    "arrowParens": "avoid"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier"
    }
  },
  "dependencies": {
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "vue": "^2.6.12",
    "sequelize": "^6.3.0",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@types/node": "^14.14.14",
    "@types/sequelize": "^4.28.9"
  }
}
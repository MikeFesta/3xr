{
  "name": "3xr",
  "version": "1.0.0",
  "description": "Backend for 3xr.com",
  "author": "Mike Festa",
  "homepage": "https://www.3xr.com",
  "license": "SPDX-License-Identifier: Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/MikeFesta/3xr.git"
  },
  "main": "index.js",
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "scripts": {
    "tsc": "node_modules/typescript/bin/tsc",
    "serve": "concurrently --kill-others \"ts-node-dev --poll --respawn --transpile-only ./app/server.ts\" \"nodemon -e scss -x 'npm run build-css'\"",
    "dev": "npm run serve",
    "build": "tsc",
    "postbuild": "cp -R ./app/email/templates ./build/app/email",
    "prod": "node_modules/typescript/bin/tsc && node ./build/app/server.js",
    "start": "node ./build/app/server.js",
    "build-css": "node-sass -o ./public/stylesheets/ ./sass/3xr.scss",
    "migration:generate": "sequelize migration:generate",
    "migrate:up": "sequelize db:migrate",
    "migrate:down": "sequelize db:migrate:undo",
    "seed:generate": "sequelize seed:generate",
    "seed:up": "sequelize db:seed:all",
    "seed:down": "sequelize db:seed:undo",
    "lint": "eslint '*/**/*.{js,ts}' --quiet --fix",
    "test": "jest",
    "format": "prettier --write ./",
    "pull-types": "./pull_types.sh && ./init_types.sh",
    "type-check": "npm run tsc -- --noEmit"
  },
  "dependencies": {
    "archiver": "^5.2.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "connect-ensure-login": "^0.1.1",
    "connect-redis": "^4.0.4",
    "cors": "^2.8.5",
    "csv-parse": "^4.15.0",
    "dotenv": "^8.2.0",
    "express": "4.16.4",
    "express-session": "^1.17.1",
    "express-validator": "^6.2.0",
    "form-data": "^3.0.0",
    "helmet": "^3.21.2",
    "joi": "^17.3.0",
    "jsdom": "^16.4.0",
    "json2csv": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.2",
    "passport": "^0.4.1",
    "passport-auth-token": "^1.0.1",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "pug": "^2.0.4",
    "redis": "^2.8.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.4",
    "sequelize": "^6.3.0",
    "serve-favicon": "^2.5.0",
    "supertest": "^5.0.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6",
    "vue": "^2.6.12",
    "vue-server-renderer": "^2.6.12",
    "winston": "^3.3.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.11.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/connect-ensure-login": "^0.1.5",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.6",
    "@types/express-session": "^1.17.0",
    "@types/helmet": "0.0.47",
    "@types/jest": "^26.0.14",
    "@types/joi": "^14.3.4",
    "@types/jsdom": "^16.2.4",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/moment-timezone": "^0.5.30",
    "@types/morgan": "^1.9.0",
    "@types/node": "^14.11.8",
    "@types/nodemailer": "^6.4.0",
    "@types/passport": "^1.0.3",
    "@types/passport-http-bearer": "^1.0.36",
    "@types/passport-local": "^1.0.33",
    "@types/redis": "^2.8.22",
    "@types/request-promise": "^4.1.46",
    "@types/sequelize": "^4.28.9",
    "@types/serve-favicon": "^2.5.0",
    "@types/validator": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "babel-jest": "^26.3.0",
    "concurrently": "^4.1.2",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.6.3",
    "jest-serializer-html-string": "^1.0.1",
    "lint-staged": "^10.2.11",
    "node-sass": "^6.0.1",
    "nodemon": "^1.19.4",
    "prettier": "^2.0.5",
    "sequelize-cli": "^6.2.0",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.1.8"
  }
}
